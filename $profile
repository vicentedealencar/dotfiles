
# Load posh-git example profile
. 'C:\tools\poshgit\dahlbyk-posh-git-3db1e6e\profile.example.ps1'

Function ss {
    foreach($file in ls) 
    {
        if($file.Name.Contains(".sln")){
            Write-Host "Starting $file"
            start $file
        }
    }
}

Function subl {
	&"C:\Program Files\Sublime Text 3\sublime_text.exe" $args
}

